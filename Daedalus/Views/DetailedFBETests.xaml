<UserControl x:Class="Daedalus.Views.DetailedFBETests"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:oxy="http://oxyplot.org/wpf"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800"
             DataContext="{StaticResource DetailedFixBar}">

    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="25*"/>
            <ColumnDefinition Width="25*"/>
            <ColumnDefinition Width="25*"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="25*"/>
            <RowDefinition Height="25*"/>
        </Grid.RowDefinitions>
        <oxy:PlotView Grid.Column="0" Grid.Row="0" Model="{Binding PlotModelDrawdownLong}"  />
        <oxy:PlotView Grid.Column="0" Grid.Row="1" Model="{Binding PlotModelDrawdownShort}"  />
        <oxy:PlotView Grid.Column="1" Grid.Row="0" Model="{Binding PlotModelReturnsLong}"  />
        <oxy:PlotView Grid.Column="1" Grid.Row="1" Model="{Binding PlotModelReturnsShort}"  />
        <oxy:PlotView Grid.Column="2" Grid.Row="0" Model="{Binding ExpectancyLong}"  />
        <oxy:PlotView Grid.Column="2" Grid.Row="1" Model="{Binding ExpectancyShort}"  />


    </Grid>
</UserControl>
