<UserControl x:Class="Daedalus.Views.FixedBarExitView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:oxyplot="clr-namespace:OxyPlot;assembly=OxyPlot.Wpf"
             xmlns:local="clr-namespace:Daedalus.Views"
             xmlns:oxy="http://oxyplot.org/wpf"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="80*"></RowDefinition>
            <RowDefinition Height="20*"></RowDefinition>
        </Grid.RowDefinitions>
        <oxy:PlotView Grid.Row="0" Model="{Binding }" Controller="{Binding }" />


    </Grid>
</UserControl>
