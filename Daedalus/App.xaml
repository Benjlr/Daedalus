<Application x:Class="Daedalus.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Daedalus"
             xmlns:ViewModels="clr-namespace:Daedalus.ViewModels"
             StartupUri="MainWindow.xaml"
             >

    <Application.Resources>
        <ViewModels:DetailedFixBarExits x:Key="DetailedFixBar" />
        <ViewModels:DetailedFixStopExits x:Key="DetailedStopBar" />
        <ViewModels:DetailedRandomExits x:Key="DetailedRandoms" />
        <ViewModels:InitTestViewModel x:Key="TestProgress" />
    </Application.Resources>
</Application>
