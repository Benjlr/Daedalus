<Application x:Class="Daedalus.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Daedalus"
             xmlns:ViewModels="clr-namespace:Daedalus.ViewModels"
             StartupUri="MainWindow.xaml" Startup="App_OnStartup"
             >

    <Application.Resources>
        <ViewModels:FixedBarExitViewmodel x:Key="FixedBarExitViewmodel" />
        <ViewModels:StopTargetExitViewmodel x:Key="StopTargetExitViewmodel" />
        <ViewModels:RandomExitViewmodel x:Key="RandomExitViewmodel" />
        <ViewModels:InitTestViewModel x:Key="TestProgress" />
    </Application.Resources>
</Application>
